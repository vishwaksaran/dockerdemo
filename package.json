  "scripts": {
    "build": "webpack --bail --progress",
    "build:react": "webpack --config webpack.config.js --bail --progress",
    "build:angular": "webpack --config angular/webpack.config.js --bail --progress",
    "server": "webpack-dev-server --history-api-fallback --inline --progress",
    "start:react": "webpack-dev-server --history-api-fallback --inline --progress --host 0.0.0.0 --port 8080",
    "start:angular": "webpack-dev-server --config angular/webpack.config.js --inline --progress --host 0.0.0.0 --port 8081",
    "test": "karma start --threads 4 --source-map=false",
    "unittests": "karma start --suite UnitTests --threads 6",
    "directivetests": "karma start --suite DirectiveTests --threads 6",
    "directivegridtests": "karma start --suite DirectiveGridTests --threads 6",
    "test:react": "jest",
    "test:react:watch": "jest --watch",
    "test:react:coverage": "jest --coverage",
    "linter": "eslint .",
    "nlv": "node-license-validator --production --allow-licenses CPL MPL BSD BSD-2-Clause BSD-3-Clause BSD-4-Clause MIT Apache Apache-2.0 \"Apache 2\" ISC WTFPl CC-BY-4.0 CC-BY-3.0 \"Public domain\" \"Public Domain\" MIT/X11 BSD-like Unlicense --allow-packages extsprintf verror"
  },
  "jest": {
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/main/javascript/v2/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 40,
        "lines": 40,
        "statements": 40
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setup-react-tests.js"
    ],
    "testEnvironment": "jsdom"
  },
